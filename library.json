{
  "name": "openimu-core-libraries",
  "version": "1.0.2",
  "description": "Open Source Embedded IMU and Navigation Library",
  "keywords": "sensors, IMU, INS, AI, automotive, navigation",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/Aceinna/openimu-core.git",
    "branch": "develop"
  },
  "build": {
    "flags": [
      "-I Common/include",
      "-I BSP/include",
      "-I BSP",
      "-I CSL",
      "-I CSL/include",
      "-I GPS/include",
      "-I GPS",
      "-I Math/include",
      "-I Platform",
      "-I Algorithm/include",
      "-I Algorithm",
      "-I FreeRtos/include",
      "-I Math",
      "-I UART/include",
      "-I Filter/include",
      "-L Platform",
      "-l libPlatform"
    ],
    "libArchive": false,
    "platforms": "aceinna_imu"
  },
     "authors": [
        {
            "maintainer": false, 
            "name": "Aceinna", 
            "url": "https://github.com/Aceinna", 
            "email": null
        }
    ], 
    "keywords": [
        "ai", 
        "automotive", 
        "ins", 
        "imu", 
        "sensors", 
        "navigation"
    ]
 }
